{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"namedInputs": {
		"default": ["{projectRoot}/**/*"]
	},
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["default", "^default"],
			"cache": true
		},
		"dev": {
			"dependsOn": ["^build"]
		},
		"test": {
			"dependsOn": ["^build"],
			"cache": true
		},
		"lint": {
			"dependsOn": ["format"],
			"cache": true
		},
		"format": {
			"cache": true
		}
	},
	"nxCloudAccessToken": "NmIwOWM4ZjUtZWNkMi00NTY1LThmZmEtNzMzZDEzOTZkYmVifHJlYWQ=",
	"affected": {
		"defaultBase": "origin/main"
	},
	"useDaemonProcess": false
}
